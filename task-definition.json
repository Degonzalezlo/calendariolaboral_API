{
  "family": "calendariolaboral-api-task-def",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::775696080147:role/ECS-task-executionRole",
  "taskRoleArn": "",

  "containerDefinitions": [
    {
      "name": "api-calendariolaboral-container",
      "image": "775696080147.dkr.ecr.us-east-1.amazonaws.com/api-calendariolaboral:1174377",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [  
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/calendariolaboral-api",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],

  "requiresCompatibilities": ["FARGATE"],

  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
